{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./",
    "baseUrl": ".",
    "module": "commonjs",
    "paths": {
      "user/*": ["user/*"],
      "freet/*": ["freet/*"],
      "community/*": ["community/*"],
      "feed/*": ["feed/*"]
    }
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  }
}
